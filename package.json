{
  "name": "Telnyx-webdriverio",
  "type": "module",
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@wdio/allure-reporter": "^9.12.6",
    "@wdio/cli": "^9.12.7",
    "@wdio/local-runner": "^9.12.7",
    "@wdio/mocha-framework": "^9.12.6",
    "@wdio/spec-reporter": "^9.12.6",
    "allure-commandline": "^2.34.0",
    "wdio-docker-service": "^3.2.1"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "npm": "^11.3.0",
    "rimraf": "^6.0.1"
  },
  "scripts": {
    "clean:allure": "rimraf ./allure-results ./allure-report",
    "test": "wdio run ./wdio.conf.js",
    "test:chrome:headless": "cross-env BROWSER=chrome HEADLESS=true wdio run wdio.conf.ts",
    "test:firefox:headless": "cross-env BROWSER=firefox HEADLESS=true wdio run wdio.conf.ts",
    "allure:generate": "npx allure generate ./allure-results --clean -o ./allure-report",
    "allure:open": "npx allure open ./allure-report",
    "docker:build": "docker build -t telnyx-tests .",
    "docker:run": "docker run --rm telnyx-tests"
  }
}
